{
  "name": "thuisbank",
  "version": "0.0.0",
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:check"
    }
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "./jest-setup.ts"
    ]
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --extra-webpack-config webpack.extras.js",
    "prettier": "prettier",
    "prettier:check": "prettier --check \"src/**/*.{ts,html}\"",
    "prettier:format": "prettier \"src/**/*.{ts,html}\" --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyze": "ng build --prod --stats-json --extra-webpack-config webpack.extras.js && webpack-bundle-analyzer dist/thuisbank/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.1",
    "@angular/common": "~8.0.1",
    "@angular/compiler": "~8.0.1",
    "@angular/core": "~8.0.1",
    "@angular/forms": "~8.0.1",
    "@angular/platform-browser": "~8.0.1",
    "@angular/platform-browser-dynamic": "~8.0.1",
    "@angular/router": "~8.0.1",
    "@fortawesome/angular-fontawesome": "^0.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@ng-bootstrap/ng-bootstrap": "^4.2.1",
    "@ngrx/entity": "^8.1.0",
    "@ngrx/store": "^8.1.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "apollo-angular": "^1.6.0",
    "apollo-angular-link-http": "^1.6.0",
    "apollo-cache-inmemory": "^1.3.2",
    "apollo-client": "^2.6.0",
    "apollo-link": "^1.2.11",
    "bootstrap": "^4.3.1",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "ngx-build-plus": "^8.0.3",
    "ngx-take-until-destroy": "^5.4.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.3",
    "@angular/compiler-cli": "~8.0.1",
    "@angular/language-service": "~8.0.1",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.134",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "jest-marbles": "^2.5.0",
    "jest-preset-angular": "^7.1.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "ng-bullet": "^1.0.3",
    "ng-mocks": "^8.0.0",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-angular": "^3.0.2",
    "typescript": "~3.4.3",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
