{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-standard",
        "tslint-react",
        "tslint-config-prettier",
        "tslint-react-hooks"
    ],
    "jsRules": {},
    "rules": {
        "react-hooks-nesting": "error",
        "interface-name": false,
        "member-ordering": false,
        "jsx-no-lambda": false
    },
    "rulesDirectory": []
}